<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="threeri.com.a3riinstitute">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Activity"></activity>
        <activity android:name=".User_Activity"></activity>
        <activity android:name=".Admin_Activity"></activity>
        <activity android:name=".Welcome_Admin_Activity"></activity>
        <activity android:name=".Welcome_User_Activity"></activity>
    </application>

</manifest>